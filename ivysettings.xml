<ivysettings>
  <settings defaultResolver="public"/>
  <resolvers>
    <ibiblio name="ibiblio" m2compatible="true"/>
    <url name="tudelft" m2compatible="true">
      <artifact pattern="https://simulation.tudelft.nl/maven/[organisation]/[artifact]/[revision]/[artifact]-[revision].[ext]"/>
    </url>
    <chain name="public">
      <resolver ref="ibiblio"/>
      <resolver ref="tudelft"/>
    </chain>
  </resolvers>
</ivysettings>
